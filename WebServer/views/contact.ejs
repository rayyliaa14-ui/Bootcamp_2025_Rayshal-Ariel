<h1>Contact List</h1>
<a href="/contact/add" class="btn btn-success">Create new contact</a>
<table class="table table-striped table-dark">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Nama</th>
      <th scope="col">Mobile</th>
      <th scope="col">Action</th>
    </tr>
  </thead>
  <tbody>
    <% contact.forEach(c => { %>
        <tr>
      <th scope="row">1</th>
      <td><%= c.name  %> </td>
      <td><%= c.mobile %></td>
        <td>
        <a href="/contact/<%= c.name %>" class="btn btn-primary">Detail</a>
        </td>
    </tr>
    <% })%>
  </tbody>
</table>

